{
	"name": "ai-stock-predictions",
	"version": "1.0.0",
	"description": "CLI application for stock price prediction using TensorFlow.js LSTM models",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"start": "node src/index.ts",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"lint": "eslint src --ext .ts",
		"knip": "knip",
		"typecheck": "tsc --noEmit",
		"format": "prettier --write src/**/*.ts",
		"format:check": "prettier --check src/**/*.ts",
		"ci": "npm run typecheck && npm run lint && npm run knip && npm run test:coverage"
	},
	"keywords": [
		"stock",
		"prediction",
		"machine-learning",
		"tensorflow",
		"cli",
		"typescript",
		"lstm",
		"yahoo-finance",
		"chart-js"
	],
	"author": "AI Stock Predictions CLI",
	"license": "MIT",
	"engines": {
		"node": ">=24.0.0"
	},
	"dependencies": {
		"@tensorflow/tfjs": "^4.22.0",
		"better-sqlite3": "^12.6.2",
		"chalk": "^5.6.2",
		"commander": "^14.0.3",
		"fs-extra": "^11.3.3",
		"ora": "^9.1.0",
		"yahoo-finance2": "^3.13.0",
		"yaml": "^2.8.2",
		"zod": "^4.3.6"
	},
	"optionalDependencies": {
		"@tensorflow/tfjs-node": "^4.22.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/better-sqlite3": "^7.6.13",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^25.2.0",
		"@vitest/browser": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-jsdoc": "^62.5.0",
		"eslint-plugin-regexp": "^3.0.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"knip": "^5.82.1",
		"prettier": "^3.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vitest": "^4.0.18"
	}
}
